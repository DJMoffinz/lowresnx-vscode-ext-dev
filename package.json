{
    "name": "lowres-nx",
    "displayName": "LowRes NX",
    "description": "Language support for LowRes NX Basic.",
    "version": "0.0.5",
    "publisher": "schraf",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/schraf/lowresnx-visualstudiocode-ext.git"
    },
    "author": "Marc Schraffenberger",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/schraf/lowresnx-visualstudiocode-ext/issues"
    },
    "homepage": "https://github.com/schraf/lowresnx-visualstudiocode-ext#readme",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "nx",
                "aliases": [
                    "LowRes NX Basic",
                    "nx"
                ],
                "extensions": [
                    ".nx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nx",
                "scopeName": "source.nx",
                "path": "./syntaxes/nx.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:nx"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.34.0",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "eslint": "^7.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "typescript": "^4.0.2"
    }
}
